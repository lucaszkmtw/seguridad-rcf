<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TGP.WSS" namespace="TGP.WSS" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Marca" table="SEG_MARCA" lazy="false" >
    <id name="Id" column="C_ID">
      <generator class="native" >
        <param name="sequence">SEG_MARCA_SQ</param>
      </generator>
    </id>
    <many-to-one name="NombreClase">
      <column name="C_ID_NOMBRE_CLASE" sql-type="NUMBER" not-null="true" />
      
    </many-to-one>
    <many-to-one name="Usuario">
      <column name="C_ID_USUARIO" sql-type="NUMBER" not-null="true" />
    </many-to-one>
    <property name="FechaRegistro">
      <column name="F_FECHA" sql-type="DATE" not-null="true" />
    </property>
    <property name="ObjetoId">
      <column name="C_OBJETO_ID" sql-type="NUMBER" not-null="true" />
    </property>
    <property name="Descripcion">
      <column name="D_DESCRIPCION" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="ColorFondo">
      <column name="D_COLOR_FONDO" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="ColorTexto">
      <column name="D_COLOR_TEXTO" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="VersionHibernate">
      <column name="N_VERSION_HIBERNATE" sql-type="NUMBER" not-null="true" />
    </property>
  </class>
</hibernate-mapping>