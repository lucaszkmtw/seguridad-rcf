<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TGP.WSS" namespace="TGP.WSS" xmlns="urn:nhibernate-mapping-2.2">
  <class name="NodoFuncional" table="SEG_NODO_FUNCIONAL">
    <id name="CId" column="C_ID">
      <generator class="native" >
        <param name="sequence">SEG_NODO_FUNCIONAL_SQ</param>
      </generator>
    </id>
    <version name="NVersionHibernate">
      <column name="N_VERSION_HIBERNATE"/>
    </version>
    <property name="DDescripcion">
      <column name="D_DESCRIPCION" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="CNegocio">
      <column name="C_NEGOCIO" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="MDescentralizado">
      <column name="M_DESCENTRALIZADO" sql-type="NUMBER" not-null="false" />
    </property>
    <property name="FUltimaOperacion">
      <column name="F_ULTIMA_OPERACION" sql-type="DATE" not-null="false" />
    </property>

    <many-to-one name="EstructuraFuncional" cascade="none" insert="false" update="false">
      <column name="C_ID_ESTRUCTURA_FUNCIONAL" not-null="true" />
    </many-to-one>
    <many-to-one name="NodoFuncionalPadre" cascade="all">
      <column name="C_ID_PADRE" not-null="false" />
    </many-to-one>
    <many-to-one name="UsuarioLogin" cascade="none">
      <column name="C_ID_USUARIO_LOGIN" not-null="false" />
    </many-to-one>
  </class>
</hibernate-mapping>