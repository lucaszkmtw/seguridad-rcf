<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TGP.WSS" namespace="TGP.WSS" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SolicitudToken" table="TGP_PARAMETRO" lazy="false" >
    <id name="CId" column="C_ID"></id>
    <property name="Nombre">
      <column name="D_NOMBRE" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="Valor">
      <column name="D_VALOR" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="Descripcion">
      <column name="D_DESCRIPCION" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="TipoDato">
      <column name="D_TIPO_DATO" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="FormatoTransf">
      <column name="D_FORMATO_TRANSF" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="Usuario">
      <column name="C_USUARIO" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="FechaAlta">
      <column name="FH_ALTA" sql-type="DATE" not-null="true" />
    </property>
    <property name="FechaModificacion">
      <column name="FHU_ACTUALIZ" sql-type="DATE" not-null="false" />
    </property>
  </class>
</hibernate-mapping>