<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TGP.WSS" namespace="TGP.WSS" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TipoUsuario" table="SEG_TIPO_USUARIO" lazy="false" >
    <id name="CId" column="C_ID">
      <generator class="native" >
        <param name="sequence">SEG_TIPO_USUARIO_SQ</param>
      </generator>
    </id>
    <version name="NVersionHibernate">
      <column name="N_VERSION_HIBERNATE" sql-type="NUMBER" not-null="true" />
    </version>
    <property name="CCodigo" lazy="false">
      <column name="C_CODIGO" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="DDescripcion">
      <column name="D_DESCRIPCION" sql-type="VARCHAR2" not-null="true" />
    </property>
    
  </class>
</hibernate-mapping>