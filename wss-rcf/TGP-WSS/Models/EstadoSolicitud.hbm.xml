<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TGP.WSS" namespace="TGP.WSS" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EstadoSolicitud" table="SEG_ESTADO_SOLICITUD" lazy="true" >
    <id name="Id" column="C_ID" />
    <property name="Codigo">
      <column name="C_CODIGO" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="Descripcion">
      <column name="D_DESCRIPCION" sql-type="VARCHAR2" not-null="true" />
    </property>
    <property name="NVersionHibernate">
      <column name="N_VERSION_HIBERNATE" sql-type="NUMBER"  />
    </property>
  </class>
</hibernate-mapping>