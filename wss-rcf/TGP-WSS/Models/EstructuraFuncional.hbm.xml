<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TGP.WSS" namespace="TGP.WSS" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EstructuraFuncional" table="SEG_ESTRUCTURA_FUNCIONAL">
    <id name="CId" column="C_ID">
      <generator class="native" >
        <param name="sequence">SEG_ESTRUCTURAFUNCIONAL_SQ</param>
      </generator>
    </id>
    <version name="NVersionHibernate">
      <column name="N_VERSION_HIBERNATE" />
    </version>
    <property name="DDescripcion">
      <column name="D_DESCRIPCION" sql-type="VARCHAR2" not-null="true" unique="true" />
    </property>
    <property name="CCodigo">
      <column name="C_CODIGO" sql-type="VARCHAR2" not-null="true" unique="true" />
    </property>
    <property name="FUltimaOperacion">
      <column name="F_ULTIMA_OPERACION" sql-type="DATE" not-null="false" />
    </property>
    <property name="MBloqueado">
      <column name="M_BLOQUEADO" not-null="true" />
    </property>
    <many-to-one name="Usuario" cascade="none">
      <column name="C_ID_USUARIO_LOGIN" not-null="false" />
    </many-to-one>
    <bag  name="Nodos" inverse="true" cascade="all-delete-orphan">
      <key column="C_ID_ESTRUCTURA_FUNCIONAL"/>
      <one-to-many class="NodoFuncional" />
    </bag>

  </class>
</hibernate-mapping>